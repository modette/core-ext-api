{
	"name": "modette/core-ext-api",
	"description": "API layer for Modette applications",
	"type": "library",
	"license": "MPL-2.0",
	"keywords": [
		"api",
		"apitte",
		"middlewares",
		"openapi",
		"swagger"
	],
	"homepage": "https://github.com/modette/cmf",
	"authors": [
		{
			"name": "Marek Barto≈°",
			"homepage": "https://github.com/mabar"
		}
	],
	"require": {
		"php": "^7.2.0",
		"apitte/console": "^0.7.0",
		"apitte/core": "^0.7.0",
		"apitte/debug": "^0.7.0",
		"apitte/middlewares": "^0.7.0",
		"apitte/negotiation": "^0.7.0",
		"apitte/openapi": "^0.7.0",
		"contributte/middlewares": "^0.9.0",
		"contributte/phpdoc": "^0.3.0",
		"contributte/psr7-http-message": "^0.6.0",
		"modette/core": "^1.0.0",
		"modette/core-ext-http": "^1.0.0",
		"symfony/validator": "^4.3.0"
	},
	"require-dev": {
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan": "^0.11.0",
		"phpstan/phpstan-nette": "^0.11.0",
		"phpstan/phpstan-strict-rules": "^0.11.0",
		"phpunit/phpunit": "^8.2.3"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"Modette\\Api\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Modette\\Api\\": "tests"
		}
	},
	"config": {
		"sort-packages": true
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	}
}
